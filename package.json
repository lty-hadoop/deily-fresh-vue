    "vue-skeleton-webpack-plugin": "^1.2.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "postcss-px2rem": "^0.3.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
